{
  "name": "eslint-codemod-utils",
  "version": "1.9.0",
  "description": "A collection of AST helper functions for more complex ESLint rule fixes.",
  "author": "Alex Hinds",
  "source": "lib/index.ts",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/DarkPurple141/eslint-codemod-utils.git"
  },
  "keywords": [
    "eslint",
    "codemods",
    "jscodeshift",
    "espree"
  ],
  "publishConfig": {
    "main": "dist/cjs/index.js",
    "module": "dist/esm/index.js",
    "typings": "dist/esm/index.d.ts"
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && tsc --project tsconfig.esm.json",
    "test": "vitest --run",
    "prepublish": "pnpm build"
  },
  "license": "ISC",
  "dependencies": {
    "@types/estree-jsx": "^1.0.0",
    "@typescript-eslint/types": "^5.45.0"
  },
  "devDependencies": {
    "@types/eslint": "^8.4.10",
    "@types/node": "^20.0.0",
    "@typescript-eslint/typescript-estree": "^5.45.0",
    "espree": "^9.3.1",
    "typescript": "^4.9.0",
    "vitest": "latest"
  },
  "exports": {
    ".": {
      "module": {
        "default": "./dist/esm/index.js"
      },
      "default": "./dist/cjs/index.js"
    },
    "./jsx-runtime": {
      "module": {
        "default": "./dist/esm/jsx-runtime.js"
      },
      "default": "./dist/cjs/jsx-runtime.js"
    }
  }
}