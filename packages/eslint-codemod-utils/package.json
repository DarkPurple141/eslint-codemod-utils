    "prepublish": "pnpm build"
  },
  "license": "ISC",
  "dependencies": {
    "@types/estree-jsx": "^0.0.1"
  },
  "devDependencies": {
    "@types/eslint": "^8.4.1",
    "@types/node": "^17.0.18",
    "espree": "^9.3.1",
    "typescript": "^4.5.5",
    "vitest": "latest"
  }
}